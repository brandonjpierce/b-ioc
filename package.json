{
  "name": "b-ioc-js",
  "version": "2.1.2",
  "description": "A magic free, dirt simple IoC container for Javascript.",
  "keywords": [
    "require",
    "dependency",
    "dependencies",
    "di",
    "dependency injection",
    "ioc",
    "inversion of control",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/brandonjpierce/b-ioc",
  "repository": {
    "type": "git",
    "url": "git://github.com/brandonjpierce/b-ioc.git"
  },
  "license": "MIT",
  "author": {
    "name": "Brandon Pierce",
    "email": "brandon@brandonjpierce.com",
    "url": "http://brandonjpierce.com"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds link && ts-swc tape test/**/*.test.cjs && tsds unlink",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/tape": "^5.6.4",
    "depcheck": "^1.4.7",
    "tape": "^5.7.5",
    "ts-dev-stack": "^0.15.8"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
